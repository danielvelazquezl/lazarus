<div class="content-heading">Editar Tipo de Movimiento</div>

<%= render 'form', movement_type: @movement_type %>

<div class="card">
  <div class="row">
    <div class="col-12 col-xl-auto">
      <%= link_to @movement_type do%>
        <button class="mb-1 btn btn-primary" type="button">Mostrar</button>
      <% end %>
    </div>
    <div class="col col-xl-2">
      <%= link_to movement_types_path do %>
        <button class="mb-1 btn btn-primary" type="button">Volver</button>
      <% end %>
    </div>
  </div>
</div>
